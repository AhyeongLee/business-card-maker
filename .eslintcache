act';\nimport logoImg from '../../images/business-cards.png';\nimport styles from './login.module.css';\nimport { Link, useHistory} from \"react-router-dom\";\nimport Loading from '../loading/loading';\n\nconst Login = (props) => {\n    const history = useHistory();\n\n    const hasSession = () => {\n        history.push('/');\n    }\n    const hasNoSession = () => {\n        props.setIsWaiting(false); \n    }\n    useEffect(() => {\n        props.loginService.checkSession(hasSession, hasNoSession);\n    }, []);\n\n    const handleLoginGoogle = () => {\n        props.setIsWaiting(true);\n        const provider = props.loginService.getGoogleAuthProvider();\n        props.loginService.signInWithRedirect(provider)\n        .then(() => {\n            history.push('/');\n        });\n    }\n    const handleLoginGithub = () => {\n        props.setIsWaiting(true);\n        const provider = props.loginService.getGithubAuthProvider();\n        props.loginService.signInWithRedirect(provider)\n        .then(() => {\n            history.push('/');\n        });\n    }\n    return (\n        <>\n        {props.isWaiting ? \n        <Loading /> :\n        <div className={styles.container}>\n            <div className={styles.glass}>\n                <div className={styles.logo}>\n                    <img className={styles.logo_image} src={logoImg} alt=\"logo image\"/>\n                    <h1 className={styles.logo_title}>Business Card Maker</h1>\n                </div>\n                <div className={styles.login_btn}>\n                    <div className={styles.btn_container} onClick={handleLoginGoogle}>\n                        <i className=\"fab fa-google-plus-g\"></i>\n                        <div>Sign in with Google</div>\n                    </div>\n                    <div className={styles.btn_container} onClick={handleLoginGithub}>\n                        <i className=\"fab fa-github\"></i>\n                        <div>Sign in with Github</div>\n                    </div>\n                </div>\n            </div>\n        </div> \n    }\n    </>\n\n)};\n\nexport default Login;","/Users/awscloud/Documents/VSCode/business-card-maker/src/components/header/header.jsx",["63","64"],"import React, { useRef, memo } from 'react';\nimport logoImg from '../../images/business-cards.png';\nimport styles from './header.module.css';\nimport { Link } from 'react-router-dom';\n\n\nconst Header = memo((props) => {\n    const addRef = useRef();\n\n    const handleAddBtn = (e) => {\n        if (e.target.closest(`#${addRef.current.id}`)) {\n            props.onAddBtn();\n            setTimeout(() => {\n                window.scroll({\n                top: document.body.scrollHeight,\n                behavior: 'smooth'\n              })}, 200);\n        }\n        \n    }\n    const handleLogout =() => {\n        props.onLogout();\n    }\n    return (\n    <header className={styles.container}>\n        <div className={styles.contents}>\n            <div ref={addRef} className={styles.add_container}  onClick={handleAddBtn} id=\"header-add-btn-container\">\n                <i className={`fas fa-plus ${styles.add_btn}`}></i>\n            </div>\n            <img className={styles.logo_image} src={logoImg} alt=\"logo\"/>\n            <h1 className={styles.logo_title}>Business Card Maker</h1>\n            \n                <div className={styles.logout_container}>\n                {/* <Link to=\"/login\" className={styles.logout_btn}> */}\n                    <a className={styles.logout_btn} onClick={handleLogout}>Logout</a>\n                    {/* Logout */}\n                {/* </Link> */}\n                </div>\n            \n        </div>\n    </header>            \n)});\n\nexport default Header;","/Users/awscloud/Documents/VSCode/business-card-maker/src/index.js",[],"/Users/awscloud/Documents/VSCode/business-card-maker/src/components/main/main.jsx",["65","66","67"],"/Users/awscloud/Documents/VSCode/business-card-maker/src/components/card_maker/c