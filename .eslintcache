(update);\n        this.firebase.database().ref('/users/' + userId + '/cards/' + cardKey).update(update);\n    }\n}\n\nexport default Database;","/Users/awscloud/Documents/VSCode/business-card-maker/src/service/cloudinary/image.js",[],{"ruleId":"72","severity":1,"message":"73","line":1,"column":17,"nodeType":"74","messageId":"75","endLine":1,"endColumn":23},{"ruleId":"72","severity":1,"message":"76","line":1,"column":25,"nodeType":"74","messageId":"75","endLine":1,"endColumn":33},{"ruleId":"72","severity":1,"message":"76","line":1,"column":28,"nodeType":"74","messageId":"75","endLine":1,"endColumn":36},{"ruleId":"72","severity":1,"message":"77","line":4,"column":10,"nodeType":"74","messageId":"75","endLine":4,"endColumn":14},{"ruleId":"78","severity":1,"message":"79","line":18,"column":8,"nodeType":"80","endLine":18,"endColumn":10,"suggestions":"81"},{"ruleId":"82","severity":1,"message":"83","line":43,"column":21,"nodeType":"84","endLine":43,"endColumn":88},{"ruleId":"72","severity":1,"message":"77","line":4,"column":10,"nodeType":"74","messageId":"75","endLine":4,"endColumn":14},{"ruleId":"85","severity":1,"message":"86","line":35,"column":21,"nodeType":"84","endLine":35,"endColumn":77},{"ruleId":"78","severity":1,"message":"87","line":24,"column":8,"nodeType":"80","endLine":24,"endColumn":10,"suggestions":"88"},{"ruleId":"89","severity":1,"message":"90","line":38,"column":26,"nodeType":"91","messageId":"92","endLine":38,"endColumn":28},{"ruleId":"93","severity":1,"message":"94","line":39,"column":17,"nodeType":"95","messageId":"96","endLine":48,"endColumn":18},{"ruleId":"72","severity":1,"message":"76","line":1,"column":25,"nodeType":"74","messageId":"75","endLine":1,"endColumn":33},{"ruleId":"72","severity":1,"message":"73","line":1,"column":17,"nodeType":"74","messageId":"75","endLine":1,"endColumn":23},{"ruleId":"72","severity":1,"message":"97","line":3,"column":8,"nodeType":"74","messageId":"75","endLine":3,"endColumn":14},{"ruleId":"93","severity":1,"message